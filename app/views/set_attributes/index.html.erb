<h1>Listing set_attributes</h1>

<table>
  <thead>
    <tr>
      <th>Setable</th>
      <th>Setable type</th>
      <th>Name</th>
      <th>Value</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @set_attributes.each do |set_attribute| %>
      <tr>
        <td><%= set_attribute.setable_id %></td>
        <td><%= set_attribute.setable_type %></td>
        <td><%= set_attribute.name %></td>
        <td><%= set_attribute.value %></td>
        <td><%= link_to 'Show', set_attribute %></td>
        <td><%= link_to 'Edit', edit_set_attribute_path(set_attribute) %></td>
        <td><%= link_to 'Destroy', set_attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Set attribute', new_set_attribute_path %>
