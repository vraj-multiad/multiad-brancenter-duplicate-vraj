<div class='col-lg-9 col-md-9 col-sm-6'>
  <%= form_tag(admin_ac_url, remote: false, id: 'admin-ac-display-form') do %>
    <strong>Keyword Search</strong><br/>
    <%= text_field_tag 'keyword', @keyword.to_s, class: 'form-control' %>
    <%= button_tag(t("__submit__"), :type => 'submit', :class => "btn btn-default submit-butt") %>
  <% end %>
</div>

<div class='col-lg-3 col-md-3 col-sm-6'>
  <%= render partial: 'admin_ac_upload' %>
</div>
<div class='col-lg-12'>&nbsp;</div>
  <%= render partial: 'admin_ac_details', locals: {ac_creator_templates: @keyword_act} %>
<br/>
<br/>


