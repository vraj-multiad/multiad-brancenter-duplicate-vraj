<h1>Listing user_uploaded_images</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image upload</th>
      <th>Filename</th>
      <th>Extension</th>
      <th>Expired</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_uploaded_images.each do |user_uploaded_image| %>
      <tr>
        <td><%= user_uploaded_image.name %></td>
        <td><%= user_uploaded_image.image_upload %></td>
        <td><%= user_uploaded_image.filename %></td>
        <td><%= user_uploaded_image.extension %></td>
        <td><%= user_uploaded_image.expired %></td>
        <td><%= link_to 'Show', user_uploaded_image %></td>
        <td><%= link_to 'Edit', edit_user_uploaded_image_path(user_uploaded_image) %></td>
        <td><%= link_to 'Destroy', user_uploaded_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User uploaded image', new_user_uploaded_image_path %>
