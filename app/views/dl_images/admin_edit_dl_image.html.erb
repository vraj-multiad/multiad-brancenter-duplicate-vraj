<%= form_tag('/admin/dl_image/load') do %>
  <input type="hidden" name="token" value="<%= @dl_image.token %>">

  <%= text_field_tag 'name', nil, :class => "form-control", :placeholder => 'name' %>
  <%= text_field_tag 'title', nil, :class => "form-control", :placeholder => 'title' %>

  <%= button_tag(t("__submit__"), :type => 'submit', :class => "btn btn-default btn-lg submit-butt") %>
<% end %>
