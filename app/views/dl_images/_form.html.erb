<%= form_for(@dl_image) do |f| %>
  <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
    <% if @dl_image.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@dl_image.errors.count, "error") %> prohibited this dl_image from being saved:</h2>

        <ul>
        <% @dl_image.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

    <div>
      <%= f.label :name %><br>
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :title %><br>
      <%= f.text_field :title, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :location %><br>
      <%= f.text_field :location, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :preview %><br>
      <%= f.text_field :preview, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :thumbnail %><br>
      <%= f.text_field :thumbnail, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :folder %><br>
      <%= f.text_field :folder, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :filename %><br>
      <%= f.text_field :filename, class: 'form-control' %>
    </div>
    <%= render partial: 'keyword/form_publish_dates' , locals: { f: f, publish_at: @dl_image.publish_at, unpublish_at: @dl_image.unpublish_at } %>
  </div>
  <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
    <%= render partial: 'set_attributes/set_attribute_list', locals: {attribute_list: @dl_image.set_attributes} %>
  </div>
  <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
    <%= render partial: 'responds_to_attributes/responds_to_attribute_list', locals: {attribute_list: @dl_image.responds_to_attributes} %>
  </div>
  <div class="actions col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <%= f.submit %>
  </div>
<% end %>
