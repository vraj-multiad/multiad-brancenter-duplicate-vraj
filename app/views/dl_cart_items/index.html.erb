<h1>Listing dl_cart_items</h1>

<table>
  <thead>
    <tr>
      <th>Dl cart</th>
      <th>Downloadable</th>
      <th>Downloadable type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @dl_cart_items.each do |dl_cart_item| %>
      <tr>
        <td><%= dl_cart_item.dl_cart_id %></td>
        <td><%= dl_cart_item.downloadable_id %></td>
        <td><%= dl_cart_item.downloadable_type %></td>
        <td><%= link_to 'Show', dl_cart_item %></td>
        <td><%= link_to 'Edit', edit_dl_cart_item_path(dl_cart_item) %></td>
        <td><%= link_to 'Destroy', dl_cart_item, method: :delete, data: { confirm: 'Are you sure?' } %> <%= link_to '/dl_cart/remove', dl_cart_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dl cart item', new_dl_cart_item_path %>
