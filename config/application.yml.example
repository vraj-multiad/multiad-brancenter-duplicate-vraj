  # ENABLE_AC_ORDER_PANEL_DOWNLOAD
  ENABLE_AC_ORDER_PANEL_DOWNLOAD_PDF: 'false'
  ENABLE_AC_ORDER_PANEL_DOWNLOAD_PNG: 'false'

  # ENABLE_FULFILLMENT_CART_DOWNLOAD
  ENABLE_FULFILLMENT_CART_DOWNLOAD_PDF: 'false'
  ENABLE_FULFILLMENT_CART_DOWNLOAD_PNG: 'false'

  # ENABLE_USER_APPROVAL
  ENABLE_USER_APPROVAL: 'false'

  # ENABLE_CONTACT_CREATE
  ENABLE_CONTACT_CREATE: 'false'

  # ENABLE_CONTACT_SHARE
  ENABLE_CONTACT_SHARE: 'false'

  # ENABLE_CONTACTS
  ENABLE_CONTACTS: 'false'

  # ENABLE_USER_REGISTRATION
  ENABLE_USER_REGISTRATION: 'true'

  # ENABLE_USER_FORGOT_PASSWORD
  ENABLE_USER_FORGOT_PASSWORD: 'true'

  # ENABLE_FACEBOOK_ID
  ENABLE_FACEBOOK_ID: 'false'

  # ENABLE_LINKEDIN_ID
  ENABLE_LINKEDIN_ID: 'false'

  # ENABLE_TWITTER_ID
  ENABLE_TWITTER_ID: 'false'

  # facebook api keys
  FACEBOOK_APP_ID: asdf

  # facebook api keys
  FACEBOOK_APP_SECRET: asdf

  # flowplayer key (specific to domain app is being served on)
  FLOWPLAYER_KEY: ABCDEF

  # gmail username
  GMAIL_USERNAME: you@multiad.com

  # gmail password
  GMAIL_PASSWORD: PASSWORD

  # kwikee api access levels 'true'/'false'
  KWIKEE_API_ACCESS_LEVELS: 'true'

  # kwikee api endpoint
  KWIKEE_API_ENDPOINT: 'https://api.kwikeesystems.com/v1.1'

  # kwikee_api password
  KWIKEE_API_PASSWORD: password

  # kwikee_api username
  KWIKEE_API_USERNAME: username

  # kwikee_api status update email recipients
  KWIKEE_API_UPDATE_EMAIL: you@multiad.com

  # KWIKEE_PRODUCT_CUSTOM_DATA_GROUPS separated by ,
  KWIKEE_PRODUCT_CUSTOM_DATA_GROUPS: 'custom_group_name,other_group_name'

  # max total search results
  MAX_RESULTS: '200'

  # max total search results
  MAX_RESULTS_AC_SEARCH: '48'

  # search page length
  MORE_AC_SEARCH_RESULTS_COUNT: '12'

  # search page length
  MORE_CATEGORY_RESULTS_COUNT: '10'

  # search page length
  MORE_SEARCH_RESULTS_COUNT: '25'

  # key for storing encrypted oauth tokens from social media
  # generate one with `ruby -e "require 'aes'; puts AES.key"`
  OAUTH_AES_KEY: asdf

  # SAML configuration
  # local authentication target from idP
  # OMNIAUTH_SAML_ASSERTION_CONSUMER_SERVICE_URL: 'https://www.mydomain.com/auth/saml/callback'

  # idP URL
  # OMNIAUTH_SAML_IDP_SSO_TARGET_URL: 'http://adfs.associatedbank.com/adfs/services/trust'

  # idP provided cert in PEM format with \n linebreaks
  # OMNIAUTH_SAML_IDP_CERT: "-----BEGIN CERTIFICATE-----\nIMIMI...=\n-----END CERTIFICATE-----\n"

  # idP provided cert fingerprint, untested
  # OMNIAUTH_SAML_IDP_CERT_FINGERPRINT: ''

  # idP provided cert fingerprint validator, untested
  # OMNIAUTH_SAML_IDP_CERT_FINGERPRINT_VALIDATOR: ''

  # application name
  # OMNIAUTH_SAML_ISSUER: 'myapp-bc'

  # SAML userid key, emailAddress in this example
  # OMNIAUTH_SAML_NAME_IDENTIFIER_FORMAT: 'urn:oasis:names:tc:SAML:1.1:nameid-format:emailAddress'

  # Custom user provision routine
  # OMNIAUTH_SAML_PROVISION_USER_ROUTINE: provision_user

  # recipients for notifications for placed orders
  ORDER_EMAIL: you@multiad.com

  # base url for MASI adcreator exports and carts
  PICKUP_URL: https://dev.ssl.multi-ad.com/bc_sandbox

  # publish notification
  PUBLISH_NOTIFICATION_EMAIL: you@multiad.com

  # publish soon duration in days
  PUBLISH_SOON_DURATION: '60'

  # rack timeout no more than 20!!!
  RACK_TIMEOUT: '15'

  # rails env
  RAILS_ENV: development

  # recipients for notifications of new registration
  REGISTRATION_EMAIL: you@multiad.com

  # REQUIRE_FIRST_NAME
  REQUIRE_FIRST_NAME: true

  # REQUIRE_LAST_NAME
  REQUIRE_LAST_NAME: true

  # REQUIRE_TITLE
  REQUIRE_TITLE: false

  # REQUIRE_ADDRESS_1
  REQUIRE_ADDRESS_1: true

  # REQUIRE_ADDRESS_2
  REQUIRE_ADDRESS_2: false

  # REQUIRE_CITY
  REQUIRE_CITY: true

  # REQUIRE_STATE
  REQUIRE_STATE: true

  # REQUIRE_ZIP_CODE
  REQUIRE_ZIP_CODE: true

  # REQUIRE_COUNTRY
  REQUIRE_COUNTRY: true

  # REQUIRE_PHONE_NUMBER
  REQUIRE_PHONE_NUMBER: true

  # REQUIRE_FAX_NUMBER
  REQUIRE_FAX_NUMBER: false

  # REQUIRE_MOBILE_NUMBER
  REQUIRE_MOBILE_NUMBER: false

  # REQUIRE_WEBSITE
  REQUIRE_WEBSITE: false

  # REQUIRE_COMPANY_NAME
  REQUIRE_COMPANY_NAME: false

  # saved_ad duration in days
  SAVED_AD_DURATION: '365'

  # ssl url for served images
  SECURE_BASE_URL: https://dev.ssl.multi-ad.com/bc_sandbox

  # sendgrid settings
  SENDGRID_API_URL: https://api.sendgrid.com/

  # sendgrid settings
  SENDGRID_USERNAME: your_sendgrid_username

  # sendgrid settings
  SENDGRID_PASSWORD: your_sendgrid_password

  # Session timeout in minutes
  SESSION_TIMEOUT_DURATION: '60'
  SESSION_TIMEOUT_WARNING_DURATION: '5'

  # Shared page duration in days
  SHARED_PAGE_DURATION: '90'

  # smtp settings
  SMTP_SETTINGS_DOMAIN: gmail.com

  # smtp settings
  SMTP_SETTINGS_ADDRESS: smtp.gmail.com

  # smtp settings
  SMTP_SETTINGS_PORT: '587'

  # smtp settings
  SMTP_SETTINGS_AUTHENTICATION: plain

  # smtp settings
  SMTP_SETTINGS_ENABLE_STARTTLS_AUTO: 'true'

  # twitter api key
  TWITTER_CONSUMER_KEY: asdf

  # twitter api key
  TWITTER_CONSUMER_SECRET: asdf

  # timezone
  TZ: America/Chicago

  # number of unicorn workers
  UNICORN_WORKER_COUNT: '1'

  # youtube api key
  YOUTUBE_API_KEY: asdf

  # youtube api key
  YOUTUBE_CLIENT_ID: asdf

  # youtube api key
  YOUTUBE_CLIENT_SECRET: asdf

  # zencoder api key
  ZENCODER_API_KEY: zencoder_api_key

  # set to a 'true' value (anything) to transcode the original video and serve it as original
  # ZENCODER_TRANSCODE_ORIGINAL: 'true'

  # audio codec to use to transcode original video
  # defaults to 'aac' if not set
  ZENCODER_TRANSCODE_ORIGINAL_AUDIO_CODEC: aac

  # format (container) to use to transcode original video
  # defaults to 'mp4' if not set
  ZENCODER_TRANSCODE_ORIGINAL_FORMAT: mp4

  # size (resolution) to use to transcode original video
  # defaults to '1920x1080' if not set
  ZENCODER_TRANSCODE_ORIGINAL_SIZE: 1920x1080

  # video codec to use to transcode original video
  # defaults to 'h264' if not set
  ZENCODER_TRANSCODE_ORIGINAL_VIDEO_CODEC: 'h264'

